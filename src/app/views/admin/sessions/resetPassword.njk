{% extends "admin/layouts/session.njk" %}

{% block head %}
    <title>Foodfy | Nova Senha</title>
{% endblock %}

{% block content %}
    <h3 class="title font-md">Nova Senha</h3>
    <form
    action="/admin/reset-password"
    method="POST"
    class="submit-form"
    data-schema="resetPassword"
    >
        {% include "includes/sessions/resetPasswordFields.njk" %}
        <div class="row">
            <button type="submit" class="button">Nova senha</button>
        </div>
    </form>
{% endblock %}
